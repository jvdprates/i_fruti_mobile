{
  "expo": {
    "name": "i-Fruti",
    "slug": "i-Fruti",
    "version": "1.0.1",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#119B15"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "br.com.ifruti.ifruti"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
